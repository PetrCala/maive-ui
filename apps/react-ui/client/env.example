# Development environment configuration
# Copy this file to .env.local and customize as needed

# R API URL for development (defaults to localhost:8787 if not set)
NEXT_PUBLIC_DEV_R_API_URL=http://localhost:8787

# Alternative: Use a remote development server
# NEXT_PUBLIC_DEV_R_API_URL=http://your-dev-server.com

# Whether to use mock data
NEXT_PUBLIC_USE_MOCK_DATA=false
